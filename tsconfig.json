{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "downlevelIteration": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "module": "esnext",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2015",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "ngx-code-scanner-detect": [
        "dist/ngx-code-scanner-detect",
        "dist/ngx-code-scanner-detect"
      ],
      "ngx-code-scanner-detect/*": [
        "dist/ngx-code-scanner-detect/*",
        "dist/ngx-code-scanner-detect/*"
      ],
      "ngx-scanner-detect": [
        "dist/ngx-scanner-detect"
      ],
      "ngx-scanner-detect/*": [
        "dist/ngx-scanner-detect/*"
      ],
      "ngx-barcode-scanner-input-detect": [
        "dist/ngx-barcode-scanner-input-detect"
      ],
      "ngx-barcode-scanner-input-detect/*": [
        "dist/ngx-barcode-scanner-input-detect/*"
      ],
      "cccc": [
        "dist/cccc"
      ],
      "cccc/*": [
        "dist/cccc/*"
      ],
      "ngx-barcodeput": [
        "dist/ngx-barcodeput"
      ],
      "ngx-barcodeput/*": [
        "dist/ngx-barcodeput/*"
      ]
    }
  }
}